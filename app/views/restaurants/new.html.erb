<h1>Add a new Restaurant</h1>



<%= simple_form_for [@restaurant] do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.select(:category, ["chinese", "italian", "japanese", "french", "belgian"]) %>
  <%= f.input :phone_number %>

  <%= f.submit "add Restaurant", class: "btn btn-primary" %>
<% end %>
